    <!-- Welcome -->
    <string name="welcome_title">Welcome!</string>
    <string name="welcome_to_sakai">Welcome to Sakai</string>
    <string name="welcome_message">
        <![CDATA[Welcome to the Sakai Demo. The Demo configuration was created as a way to let you get an instance of Sakai up and running quickly and easily.
        For more information about installing a Sakai solution appropriate for production needs please read the readme files, and visit the Sakai website at
        <a href="http://www.sakaiproject.org">sakaiproject.org</a>, and the Sakai Developer\'s site at <a href="http://collab.sakaiproject.org">collab.sakaiproject.org</a>.]]>
    </string>

    <!-- About -->
    <string name="about_sakai_title">About Sakai</string>
    <string name="about_sakai_message">
        <![CDATA[Sakai is a set of software tools designed to help instructors, researchers, and students create websites for collaboration. Using a web browser, users
        choose from a set of features to create a site that meets their needs.<br /><br />Here are some common uses of Sakai:<br /><ul><li>A researcher can create a website for
        collaboration on a project with colleagues.</li><li>An instructor can create a website that serves as an online discussion board for students to collaborate on an
        assignment.</li><li>A student can create a site for a study group or for extra curricular activities (depending on institutional policy).</li></ul><br />Sakai is a
        part of the <a href="http://www.apereo.org">Apereo Foundation</a> (www.apereo.org).<br /><br />An overview of Sakai can be found at
        <a href="http://www.sakaiproject.org">http://www.sakaiproject.org</a>. For more information, contact <a href="mailto:sakaicoordinator@apereo.org">the
        Sakai Community Coordinator</a>.]]>
    </string>

    <!-- Features -->
    <string name="features_title">Capabilities</string>
    <string name="features_capabilities">Sakai offers a variety of capabilities for use in course and project sites, including:</string>
    <string name="home_capabilities">View recent announcements, discussions, and chat activity</string>
    <string name="announcements_capabilities">Post current, time-critical information</string>
    <string name="assignments_capabilities">Post assignments and receive submissions online</string>
    <string name="basiclti">Basiclti</string>
    <string name="basiclti_capabilities">Launch and exchange information with externally-hosted tools</string>
    <string name="chat_room_capabilities">Participate in real-time, written conversations</string>
    <string name="drop_box_capabilities">Allow private file-sharing between instructors and students</string>
    <string name="entity_broker">Entity Broker</string>
    <string name="entity_broker_capabilities">Provides a set of RESTful APIs that support interoperability between Sakai tools.</string>
    <string name="email_archive_capabilities">Maintain an archive of all email sent to the worksite\'s email list</string>
    <string name="gradebook_capabilities">Compute and store grades</string>
    <string name="help_capabilities">Access help documentation</string>
    <string name="lessons_capabilities">
        Can organize the course by unit, module, week, topic, or any other grouping the instructor creates. Lessons leverages the power of other Sakai tools like Assignments,
        and Test and Quizzes.
    </string>
    <string name="membership_capabilities">Choose sites in which you would like to participate</string>
    <string name="messages_and_forums">Messages and\nForums</string>
    <string name="messages_and_forums_capabilities">Create discussion forums and communicate between site groups and users using internal mail.</string>
    <string name="news_capabilities">Display an RSS feed from an external site</string>
    <string name="podcasts_capabilities">View podcasts</string>
    <string name="polls_capabilities">Create site-based surveys and polls</string>
    <string name="portofolios">Portfolios</string>
    <string name="portofolios_capabilities">Supports portfolio-based activities with a suite of tools.</string>
    <string name="postem_capabilities">Provides functionality for uploading .csv formatted files to display feedback (e.g., comments, grades) to site participants.</string>
    <string name="preferences_capabilities">Set your personal preferences, such as time zone, tab order</string>
    <string name="profile_capabilities">Provides a Facebook-style interface for editing user profiles; includes Twitter integration</string>
    <string name="quartz_scheduler">Quartz\nScheduler</string>
    <string name="quartz_scheduler_capabilities">Schedule and run cron jobs</string>
    <string name="resources_capabilities">Add documents and URLs to your worksite</string>
    <string name="roster_capabilities">View list of site participants</string>
    <string name="calendar_capabilities">Keep track of important dates and deadlines on worksite calendar</string>
    <string name="search_capabilities">Provides search capabilities of Sakai content</string>
    <string name="section_info_capabilities">Manage sections or groups within a site</string>
    <string name="shortened_url_service">Shortened URL\nService</string>
    <string name="shortened_url_service_capabilities">
        A simple service that allows tools and services to shorten links to anything, either via the Java-based service or the Entity Provider.
    </string>
    <string name="signup_tool">Signup tool</string>
    <string name="signup_tool_capabilities">Schedule instructor and ta office hours, study groups, and review sessions.</string>
    <string name="site_info_capabilities">View worksite profile and participants list</string>
    <string name="site_stats">Site Stats</string>
    <string name="site_stats_capabilities">View site usage statistics</string>
    <string name="syllabus_capabilities">Create a worksite syllabus</string>
    <string name="test_quizzes_capabilities">Create, administer, and correct tests and quizzes online, and automatically feed the results to the Gradebook</string>
    <string name="web_content_capabilities">Include external website content in your worksite</string>
    <string name="wiki_capabilities">Collaboratively edit simple web pages</string>
    <string name="worksite_setup">Worksite Setup</string>
    <string name="worksite_setup_capabilities">Create and manage sites</string>
    <string name="webservices">Webservices</string>
    <string name="webservices_capabilities">Includes a rich collection of web services for system admins</string>

